import random

print('This program picks a random number between 1 and 100 and tells you'
      'if you guess is to high or low until guess it.')

num = random.randint(1, 100)

guessing = True
while guessing:
      guess = int(input('What is your guess? -> '))
      if guess > num:
          print('To high!')
      elif guess < num:
            print('Too low!')
      else:
            print('Nice, you got it!')
            guessing = False
            
